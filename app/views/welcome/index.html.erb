            <a class="logo-content" href="/">
              <%= image_tag "logo.png", class: "img-responsive logo" %>
            </a> 
          </div>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li class="active"><a href="#" data-nav-section="home"><span>Inicio</span></a></li>
            <li><a href="#" data-nav-section="us"><span>Nosotros</span></a></li>
            <li><a href="#" data-nav-section="services"><span>Servicios</span></a></li>
            <li><a href="#" data-nav-section="contact-us"><span>Contactenos</span></a></li>
            <li class="call-to-action area"><a href="http://panel.stcarga.com.co" target="_blank"><span>Area de empleados</span></a></li>
          </ul>
        </div>
      </nav>
    </div>
